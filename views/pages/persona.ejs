<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="container">

    <header>
        <%- include('../partials/header'); %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>Persona</h1>
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">User</th>
                    <th scope="col">Name</th>
                    <th scope="col">Surname</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <th scope="row"><%= persona.id_persona %></th>
                    <td><%= persona.user %></td>
                    <td><%= persona.name %></td>
                    <td><%= persona.surname %></td>
                  </tr>
                </tbody>
              </table>
        </div>
    </main>

    <footer>
        <%- include('../partials/footer'); %>
    </footer>

</body>

</html>